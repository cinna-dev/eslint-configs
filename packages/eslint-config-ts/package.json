{
  "name": "@cinna-dev/eslint-config-ts",
  "version": "1.0.39-alpha.0",
  "type": "module",
  "description": "eslint config for typescript",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsup --dts --minify --format esm,cjs src/index.ts",
    "create": "bash ../../scripts/remove-deactivated-rules.sh src/index.ts ./src/config.js"
  },
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "typescript"
  ],
  "author": "cinna-dev",
  "license": "ISC",
  "peerDependencies": {
    "@eslint/js": ">=9",
    "@types/eslint__js": ">=8",
    "@typescript-eslint/eslint-plugin": ">=7",
    "@typescript-eslint/parser": ">=7",
    "eslint": ">=9",
    "globals": ">=15",
    "typescript": ">=5",
    "typescript-eslint": ">=7"
  },
  "gitHead": "ab42ba9e8c1f688bff6cf6b2b9d8fb23fe9693db"
}
