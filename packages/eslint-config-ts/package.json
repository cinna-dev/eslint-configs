{
  "name": "@cinna-dev/eslint-config-ts",
  "version": "1.0.39-alpha.0",
  "type": "commonjs",
  "description": "eslint config for typescript",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "terser src/index.js -o dist/index.js",
    "create": "bash ../../scripts/remove-deactivated-rules.sh ./src/index.js ./src/config.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "typescript"
  ],
  "author": "cinna-dev",
  "license": "ISC",
  "peerDependencies": {
    "@eslint/js": ">=9",
    "@types/eslint__js": ">=8",
    "@typescript-eslint/eslint-plugin": ">=7",
    "@typescript-eslint/parser": ">=7",
    "eslint": ">=9",
    "globals": ">=15",
    "typescript": ">=5",
    "typescript-eslint": ">=7"
  }
}
