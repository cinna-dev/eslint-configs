{
  "name": "@cinna-dev/eslint-config-error",
  "version": "1.0.22-alpha.0",
  "type": "module",
  "description": "eslint config finding potential errors",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup --dts --minify --format esm,cjs src/index.ts"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs"
  },
  "keywords": [
    "eslint",
    "eslint config",
    "error",
    "linter"
  ],
  "peerDependencies": {
    "eslint": ">=9"
  },
  "author": "cinna-dev",
  "license": "ISC"
}
