{
  "name": "@cinna-dev/eslint-config-error",
  "version": "1.0.23-alpha.0",
  "type": "module",
  "description": "eslint config finding potential errors",
  "scripts": {
    "build": "tsdown --dts --minify --format esm,cjs src/index.ts"
  },
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "keywords": [
    "eslint",
    "eslint config",
    "error",
    "linter"
  ],
  "peerDependencies": {
    "eslint": ">=9"
  },
  "author": "cinna-dev",
  "license": "ISC"
}
