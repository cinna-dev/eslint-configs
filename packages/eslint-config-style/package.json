{
  "name": "@cinna-dev/eslint-config-style",
  "version": "1.0.36-alpha.0",
  "type": "module",
  "description": "eslint config",
  "scripts": {
    "build": "tsup --dts --minify --format esm,cjs src/index.ts src/basic.ts"
  },
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "import/basic": "./dist/basic.js",
    "require/basic": "./dist/basic.cjs"
  },
  "keywords": [
    "eslint",
    "eslint config",
    "style",
    "linter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cinna-dev/eslint-config-style.git"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "author": "cinna-dev",
  "license": "ISC"
}
